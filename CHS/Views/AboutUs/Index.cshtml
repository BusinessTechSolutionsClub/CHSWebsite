            <img class="team-photo"src="~/Images/rsz_13.jpg" />
          
        </div>
    </div>
    <hr style="border-color:black;" />
    <div class="row">
        <div class="col-sm-2">
            <img class="individual-photo" src="~/Images/1.jpg" />
        </div>
        <div class="col-sm-10">
            <p>@ViewBag.YoungBio</p>
        </div>
    </div>
    <br /><br />
    <div class="row">
        <div class="col-sm-2">
            <img class="individual-photo" src="~/Images/2.jpg" />
        </div>
        <div class="col-sm-10">
            <p>@ViewBag.KristinaBio</p>
        </div>
    </div>
</div>

