
@model IEnumerable<CHS.Models.AboutUs>

@{
    ViewBag.Title = "About Us";
}

<!--Included custom css file-->
<link href="~/Content/CHSStyles.css" rel="stylesheet" />

<h1 class="page-title">About Us</h1>
<hr style="border-color:black;" />
<div class="container-fluid about">
    <div class="row">
